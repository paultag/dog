import sys
import os
import pydog.dog as ccat

f = sys.argv[1]

c = ccat.ccat()

fp, xtn = os.path.splitext( f )
xtn = xtn[1:]

c.set_colors_by_name( "control" ) # xtn )
c.cat( f )
